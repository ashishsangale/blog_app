<%- include('./partials/header') %> 

<% blogs.forEach(function(blog){ %>
    <div>
        <h1><%= blog.title %> </h1>
        <img src="<%= blog.image %> ">
        <p><%= blog.body.substring(0, 50) %>....... </p>
        <span><%= blog.created %> </span>
        <a href="/blogs/<%= blog._id %> ">Read more</a>
    </div> 
<% }) %> 

<%- include('./partials/footer') %> 